CREATE FOREIGN TABLE update_test (
    id  serial OPTIONS (key 'true'),
	  a   INT DEFAULT 10,
    b   INT,
    c   TEXT
) SERVER :DB_SERVERNAME OPTIONS ( table_name 'update_test');
